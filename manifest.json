{
  "name": "Test Screenshot Extension",
  "version": "1.3",
  "description": "Demonstrate screenshot functionality in the chrome.tabs api.",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at":"document_end"
    }
  ],
  "browser_action": {
    "default_icon": "camera.png",
    "default_title": "Take a screen shot!",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "<all_urls>"
  ],
  "manifest_version": 2
}
