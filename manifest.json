{
  "manifest_version": 2,

  "name": "Edit",
  "description": "This extension offers a tool to take a screenshot and edit the screenshot",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "desktopCapture"
  ]
}